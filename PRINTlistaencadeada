#include <stdio.h>
#include <stdlib.h>

typedef struct datanode{
      int id;

}DataNode;

typedef struct node{
      DataNode data;
      struct node* next;

}Node;

typedef struct list{
      int size;
      Node* head;
}List;

List* creatlist();
void push(List* list, DataNode data);
void printList(List* list);

List* creatlist(){
      List* list = (List*) malloc(sizeof(List));

      list->size = 0;
      list->head = NULL;

      return list;
}

void push(List* list, DataNode data){
      Node* node = (Node*) malloc(sizeof(Node));
      node->data = data;
      node->next = list->head;
      list->head = node;
      list->size++;
}

void printList(List* list){
      Node* pointer = list->head;
      
      if(pointer == NULL)
      {
      printf("Lista Vazia");
      }

      while(pointer != NULL)
      {
        printf("%d ", pointer->data.id);
        pointer = pointer->next;
      }

}

int main()
{
     List* l = creatlist();
     
     DataNode data;

     data.id = 5;
     push(l, data);

     data.id = 9;
     push(l, data);
     
     data.id = 4;
     push(l, data);
     
     data.id = 6;
     push(l, data);
     
     data.id = 2;
     push(l, data);

     printList(l);
}
